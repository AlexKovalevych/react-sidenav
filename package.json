{
  "name": "react-sidenav",
  "version": "1.0.3",
  "description": "A Simple SideBar Navigation written in React",
  "main": "dist/index.js",
  "author": "Warren Mira",
  "main": "lib/index",
  "scripts": {
    "test-watch": "mocha --require test/hook --recursive --compilers js:babel-register -w",
    "test": "mocha --require test/hook --recursive --compilers js:babel-register",
    "build": "webpack src/index.js dist/react-sidenav.js",
    "build-min": "NODE_ENV=production COMPRESS=1 webpack src/index.js dist/react-sidenav.min.js",
    "watch": "webpack src/sidenav.js js/react-sidenav.js --watch",
    "build-cjs": "babel -d dist/ src/",
    "dist": "npm run build && npm run build-min",
    "dev": "webpack-dev-server --port 8080 --config webpack.config.js --watch dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wmira/react-sidenav"
  },
  "bugs": {
    "url": "https://github.com/wmira/react-sidenav/issues"
  },
  "homepage": "https://github.com/wmira/react-sidenav",
  "keywords": [
    "react",
    "react-component",
    "menu",
    "vertical menu",
    "navigation"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.5.0",
    "classnames": "^1.2.2",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "postcss-loader": "^0.8.1",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "should": "^8.2.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/__tests__/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
